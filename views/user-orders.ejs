
<style>
tr:nth-child(even){background-color: #f2f2f2}
</style>

<table class="table table-bordered">
    <tr class="info">
        <th>订单号</th>
        <th> 总价格（美元） </th>
        <th> 服务类型 </th>
        <th> 服务提供方 </th>
        <th> 服务起始时间 </th>
        <th> 服务结束时间 </th>
        <th>  </th>
    </tr>
    <% for(var idx=0; idx < myOrders.length; idx++) { %>
    <tr>
        <td><%= myOrders[idx].id %></td>
        <td><%= myOrders[idx].totalPrice %>  </td>
        <td><%= myOrders[idx].type %></td>
        <td><%= myOrders[idx].serverHost %>   </td>
        <td><%= myOrders[idx].serviceStartDate %> </td>
        <td><%= myOrders[idx].serviceEndDate %>    </td>
        <td><a href="/HostManager/userComment/<%= myOrders[idx].id %>">ping jia</a>    </td>
    </tr>
    <% }%>
</table>




















